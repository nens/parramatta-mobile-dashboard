{
  "name": "Parramatta-Mobile-Dashboard",
  "version": "0.0.3",
  "description": "Mobile site to view Lizard data as simple tiles",
  "main": "src/index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.production.config.js -p && cp index.html dist/",
    "extract": "i18next src/components -o locales -r -l en,nl",
    "precommit": "lint-staged",
    "release": "buck-trap -a",
    "start": "node server.js",
    "storybook": "babel-node storybook.js",
    "staging-deploy": "read -p 'Version? (see above) ' V && test $V && ansible-playbook -i deploy/hosts -K deploy/deploy.yml --limit=staging --extra-vars=\"sitename=staging.nxt.lizard.net version=v$V\"",
    "production-deploy": "read -p 'Version? (see above) ' V && test $V && ansible-playbook -i deploy/hosts -K deploy/deploy.yml --limit=production --extra-vars=\"sitename=nxt.lizard.net version=v$V\""
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.jsx": [
      "prettier --write",
      "git add"
    ]
  },
